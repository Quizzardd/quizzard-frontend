{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "lf",
  "jsxBracketSameLine": false,
  "bracketSameLine": false,
  "quoteProps": "as-needed",
  "proseWrap": "preserve"
}
